<div>
<h1>New User</h1>

<%=form_for @user do |t| %>
<%=t.label :username%>
<%=t.text_field :username%>
<%=t.label :email%>
<%=t.email_field :email%>
<%=t.label :password%>
<%=t.password_field :password%>
<%=t.submit%>


<%end%>
<% if flash[:errors] %>
    <% flash[:errors].each do |t| %>
        <p><%=t%></p>
    <%end%>
<%end%>

</div>
